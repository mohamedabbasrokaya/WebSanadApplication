@{
    ViewData["Title"] = "Index";
}
<label> الحساب: </label>

<input  type="text" id="AccountSearchs">
<br>
<span class="text-danger"></span>
                          @section scripts{
       

        <script>
            $(document).ready(function(){
            $("#AccountSearchs").autocomplete({
                source:function(request,response){
                    $.ajax({
                            url: "@Url.Action("DataTableSearch","Setting")?SEARCH_CODE=" + request.term,
                            dataType:"json",
                            success:function(data){
                                response(data)
                            }
                    });
                }
            })
            });
        </script>
                          }